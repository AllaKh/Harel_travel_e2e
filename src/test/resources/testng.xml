<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Harel Travel E2E Suite" verbose="1" parallel="false">
    <test name="Wizard Happy Path">
        <classes>
            <class name="com.tests.TravelFlowTest"/>
        </classes>
    </test>
</suite>

<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="TravelInsuranceSuite" parallel="tests" thread-count="2">
<test name="ChromeTests">
    <parameter name="browser" value="chrome"/>
    <classes>
        <class name="tests.TravelFlowTest"/>
    </classes>
</test>
<test name="FirefoxTests">
    <parameter name="browser" value="firefox"/>
    <classes>
        <class name="tests.TravelInsuranceTest"/>
    </classes>
</test>
</suite>
